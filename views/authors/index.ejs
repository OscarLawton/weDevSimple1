All The Authors
<h2>Search Authors</h2>
<form action="/authors" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% for(var x = 0; x < authors.length; x++){ %>
    <%= authors[x].name %>
    <br>
    <a href="/authors/<%= authors[x].id %>">View</a>
    <a href="/authors/<%= authors[x].id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', {url: `/authors/${authors[x].id}` }) %>
 <% } %>